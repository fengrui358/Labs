<Window x:Class="Jlion.Process.Target.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jlion.Process.Target.Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="获取基本信息" Click="btnInfo_Click" Name="btnInfo" HorizontalAlignment="Left" Margin="80,30,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox HorizontalAlignment="Left" Name="txtInfo" Height="180" Margin="80,75,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="300"/>
        <Button Content="获取完整信息" Name="btnComplateInfo" Click="btnComplateInfo_Click" HorizontalAlignment="Left" Margin="190,30,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
